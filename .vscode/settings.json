{
  "sqltools.connections": [
    {
      "previewLimit": 50,
      "driver": "SQLite",
      "name": "db.sqlite3",
      "database": "${workspaceFolder:keys4}/app/db.sqlite3"
    }
  ],
  "sqltools.useNodeRuntime": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.workingDirectories": ["./app", "./web"],
  "editor.formatOnSave": true
}
